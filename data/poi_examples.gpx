<?xml version="1.0" encoding="utf-8"?>
<gpx
 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns="http://www.topografix.com/GPX/1/0" 
 xsi:schemaLocation="http://www.topografix.com/GPX/1/0 http://www.topografix.com/GPX/1/0/gpx.xsd"
 version="1.0"
 creator="GPSDrive-Geoinfo - www.gpsdrive.cc">

<name>POI Examples</name>
<desc>Example Points-of-Interest for GPSDrive</desc>
<author>d.s.e</author>
<email>d.s.e (at) sordidmusic.com</email>
<url>www.gpsdrive.cc</url>
<urlname>GPSDrive Geoinfo-Database</urlname>
<time>2006-12-30</time>


<!-- General Structure: -->

<wpt lat="LAT" lon="LON">
  <ele>ALTITUDE</ele>
  <name>NAME</name>
  <desc>NAME</desc>
  <cmt>COMMENT</cmt>
  <src>SOURCE-NAME</src>
  <time>LAST_MODIFIED</time>
  <url>URL</url>
  <sym>POI_TYPE_ID</sym>
  <type>POI_TYPE</type>
  <proximity>PROXIMITY</proximity>
  <private>PRIVATE</private>
  <poi_extra>
    additional info stored in database table poi_extra
  </poi_extra>
</wpt>


<!-- Example-POI: -->

<wpt lat="48.13698" lon="11.52">
  <name>Test-POI</name>
  <desc>Test-POI</desc>
  <cmt>I am the Test-Point</cmt>
  <src>Example</src>
  <time>2006-10-20</time>
  <sym>waypoint</sym>
  <type>waypoint.wptgreen</type>
  <poi_extra>
  </poi_extra>
</wpt>

</gpx>

