msgid "-T        do some internal unit tests (don't start gpsdrive)\n"
msgstr "-T        chạy một số phép thử nội bộ (không khởi chạy gpsdrive)\n"

#: src/gpsdrive.c:2706
msgid "-e        use Festival-Lite (flite) for speech output\n"
msgstr "-e\tdùng Festival-Lite (flite) để xuất tiếng nói\n"

#: src/gpsdrive.c:2707
msgid "-o thing  serial device, pty master, or file to use for NMEA output\n"
msgstr "-o cái\tthiết bị nối tiếp, pty chủ hay tập tin cho kết xuất NMEA\n"

#: src/gpsdrive.c:2708
msgid "-f server select friends server, e.g. friendsd.gpsdrive.de\n"
msgstr "-f máy_phục_vụ\tchọn máy phục vụ bạn bè, v.d. « friendsd.gpsdrive.de »\n"

#: src/gpsdrive.c:2710
msgid "-X        use DBUS for communication with gpsd; this disables socket communication\n"
msgstr "-X        dùng DBUS để liên lạc với trình nền gpsd, cũng tắt liên lạc qua ổ cắm\n"

#: src/gpsdrive.c:2712
msgid ""
"-l lang.  select language of the voice output;\n"
"          language may be 'english', 'spanish', or 'german'\n"
msgstr ""
"-l ngôn_ngữ\t\tchọn ngôn ngữ của tiếng nói:\n"
"\t• english\t\ttiếng Anh\n"
"\t• spanish\t\ttiếng Tây Ban Nha\n"
"\t• german\t\ttiếng Đức\n"

#: src/gpsdrive.c:2714
msgid "-g geom.  set window geometry, e.g. 800x600\n"
msgstr "-g SỐxSỐ  đặt các chiếu của cửa sổ, v.d. 800x600\n"

#: src/gpsdrive.c:2715
msgid "-1        have only 1 button mouse, for example using touchscreen\n"
msgstr "-1\tdùng con chuột có chỉ một nút, v.d. dùng màn hình sờ\n"

#: src/gpsdrive.c:2716
msgid "-a        display APM stuff (battery status, temperature)\n"
msgstr "-a        hiển thị thông tin APM (trạng thái pin, nhiệt độ)\n"

#: src/gpsdrive.c:2717
msgid "-b server servername for NMEA server (if gpsd runs on another host)\n"
msgstr "-b TÊN\ttên máy phục vụ NMEA (nếu gpsd chạy trên máy khác)\n"

#: src/gpsdrive.c:2718
msgid "-c WP     set start position in simulation mode to this waypoint (WP)\n"
msgstr "-c ĐIỂM\tđặt vị trí bắt đầu ở chế độ mô phỏng thành điểm nút địa danh này\n"

#: src/gpsdrive.c:2719
msgid "-M mode   set GUI mode; mode may be 'desktop' (default), 'pda' or 'car'\n"
msgstr ""
"-M CHÊ_ĐỘ   đặt chế độ giao diện:\n"
"\t• desktop\tmáy tính để bàn (mặc định)\n"
"\t• pda\t\tmáy cầm tay\n"
"\t• car\t\txe\n"

#: src/gpsdrive.c:2720
msgid "-i        ignore NMEA checksum (risky, only for broken GPS receivers\n"
msgstr "-i\tbỏ qua tổng kiểm NMEA (nguy hiểm, chỉ cho máy nhận GPS bị hỏng)\n"

#: src/gpsdrive.c:2721
msgid "-q        disable SQL support\n"
msgstr "-q\ttắt hỗ trợ SQL\n"

#: src/gpsdrive.c:2722
msgid "-F        force display of position even it is invalid\n"
msgstr "-F\tbuộc hiển thị vị trí thậm chí nếu nó không hợp lệ\n"

#: src/gpsdrive.c:2723
msgid "-s        don't show splash screen\n"
msgstr "-s        không hiển thị màn hình giới thiệu\n"

#: src/gpsdrive.c:2724
msgid "-S path   take auto screenshots of different window (don't touch gpsdrive!)\n"
msgstr ""
"-S đường_dẫn\t\ttự động chụp ảnh màn hình của cửa sổ khác\n"
"\t\t\t\t(không sửa đổi gpsdrive)\n"

#: src/gpsdrive.c:2725
msgid "-P        start in Position Mode\n"
msgstr "-P        khởi chạy ở Chế độ Vị trí\n"

#: src/gpsdrive.c:2726
msgid "-W X      switch WAAS/EGNOS on (X=1), or off (X=0)\n"
msgstr ""
"-W X      đặt trạng thái WAAS/EGNOS:\n"
"\t• 1\t\tđã bặt\n"
"\t• 0\t\tbị tắt\n"

#: src/gpsdrive.c:2727
msgid "-H ALT    correct the altitude by adding this value (ALT) to it\n"
msgstr "-H CAO\t sửa cao độ bằng cách cộng giá trị này (CAO)\n"

#: src/gpsdrive.c:2728
msgid "-C file   use this config file\n"
msgstr "-C TẬP_TIN   dùng tập tin cấu hình này\n"

#: src/gpsdrive.c:2743
msgid "Select a track file"
msgstr "Chọn tập tin vết"

