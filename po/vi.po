msgstr "-T        chạy một số việc thử nội bộ (đừng khởi chạy gpsdrive)\n"

#: src/gpsdrive.c:2740
msgid "-e        use Festival-Lite (flite) for speech output\n"
msgstr "-e\tdùng Festival-Lite (flite) để nói kết xuất\n"

#: src/gpsdrive.c:2741
msgid "-o        serial device, pty master, or file for NMEA *output*\n"
msgstr "-o\tthiết bị nối tiếp, pty chủ, hay tập tin cho kết xuất NMEA\n"

#: src/gpsdrive.c:2742
msgid "-f X      Select friends server, X is i.e. friendsd.gpsdrive.de\n"
msgstr "-f X\tChọn máy phục vụ bạn bè X, v.d. « friendsd.gpsdrive.de »\n"

#: src/gpsdrive.c:2744
msgid "-X        Use DBUS for communication with gpsd. This disables serial and socket communication\n"
msgstr ""
"-X        Dùng DBUS để liên lạc với trình nền gpsd.\n"
"\t\t(Cũng tắt khả năng liên lạc qua cổng nối tiếp hay ổ cắm)\n"

#: src/gpsdrive.c:2746
msgid ""
"-l LANG   Select language of the voice,\n"
"          LANG may be english, spanish or german\n"
msgstr ""
"-l NGÔN_NGỮ  Chọn ngôn ngữ của tiếng nói.\n"
"\tNGÔN_NGỮ có thể là tiếng Anh, Tây Ban Nha hay Đức\n"

#: src/gpsdrive.c:2748
msgid ""
"-s HEIGHT set height of the screen, if autodetection\n"
"          don't satisfy you, X is i.e. 768,600,480,200\n"
msgstr ""
"-s CAO  đặt độ cao của màn hình, nếu không tự động\n"
"phát hiện được. Giá trị có dạng như 768,600,480,200\n"

#: src/gpsdrive.c:2750
msgid "-r WIDTH  set width of the screen, only with -s\n"
msgstr "-r RỘNG  đặt độ rộng của màn hình (cũng cần « -s »)\n"

#: src/gpsdrive.c:2751
msgid "-1        have only 1 button mouse, for example using touchscreen\n"
msgstr "-1\tdùng con chuột có chỉ một cái nút, v.d. dùng màn hình sờ\n"

#: src/gpsdrive.c:2752
msgid "-a        display APM Stuff ( battery status, Temperature)\n"
msgstr "-a        hiển thị thông tin APM (trạng thái pin, nhiệt độ)\n"

#: src/gpsdrive.c:2753
msgid "-b Server Servername for NMEA server (if gpsd runs on another host)\n"
msgstr "-b TÊN\tTên máy phục vụ NMEA (nếu gpsd chạy trên máy khác)\n"

#: src/gpsdrive.c:2754
msgid "-c WP     set start position in simulation mode to waypoint name WP\n"
msgstr "-c ĐIỂM\tđặt vị trí đầu ở chế độ mô phỏng thành tên địa điểm này\n"

#: src/gpsdrive.c:2755
msgid "-x        create separate window for menu\n"
msgstr "-x\ttạo cửa sổ riêng cho trình đơn\n"

#: src/gpsdrive.c:2756
msgid "-M mode   set guimode to desktop, pda or car\n"
msgstr ""
"-M CHÊ_ĐỘ   đặt chế độ giao diện (guimode) thành:\n"
" • desktop\tmáy tính để bàn\n"
" • pda\t\tmáy cầm tay\n"
" • car\t\txe\n"

#: src/gpsdrive.c:2757
msgid "-i        ignore NMEA checksum (risky, only for broken GPS receivers\n"
msgstr "-i\tbỏ qua tổng kiểm NMEA (nguy hiểm, chỉ cho bộ nhận GPS bị hỏng)\n"

#: src/gpsdrive.c:2758
msgid "-q        disable SQL support\n"
msgstr "-q\ttắt hỗ trợ SQL\n"

#: src/gpsdrive.c:2759
msgid "-F        force display of position even it is invalid\n"
msgstr "-F\tbuộc hiện vị trí thậm chí nếu nó không hợp lệ\n"

#: src/gpsdrive.c:2760
msgid "-S        don't show splash screen\n"
msgstr "-S        đừng hiển thị màn hình giới thiệu\n"

#: src/gpsdrive.c:2761
msgid "-P        start in Pos Mode\n"
msgstr "-P        khởi chạy ở chế độ Vị trí\n"

#: src/gpsdrive.c:2762
msgid "-W x      set x to 1 to switch WAAS/EGNOS on, set to 0 to switch off\n"
msgstr "-W x      đặt x thành 1 để bật WAAS/EGNOS, đặt thành 0 để tắt\n"

#: src/gpsdrive.c:2763
msgid "-H ALT    correct altitude, adding this value (ALT) to altitude\n"
msgstr "-H CAO\t sửa cao độ (thêm giá trị nào vào cao độ)\n"

#: src/gpsdrive.c:2764
msgid "-C file   set config file (--config-file)\n"
msgstr "-C TẬP_TIN   đặt tập tin cấu hình (--config-file)\n"

