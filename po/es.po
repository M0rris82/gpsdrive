msgstr "-X        Usa DBUS para comunicarse con gpsd. Deshabilita las comunicaciones socket y serie\n"

#: src/gpsdrive.c:2746
msgid ""
"-l LANG   Select language of the voice,\n"
"          LANG may be english, spanish or german\n"
msgstr ""
"-l LANG   Selecciona idioma para la voz,\n"
"      LANG puede ser inglés, español o alemán\n"

#: src/gpsdrive.c:2748
msgid ""
"-s HEIGHT set height of the screen, if autodetection\n"
"          don't satisfy you, X is i.e. 768,600,480,200\n"
msgstr ""
"-s ALTO ajusta el tamaño de la pantalla, si la autodetección,\n"
" no te satisface, ALTO es p. ej. 768,600,480,200\n"

